  "description": "Post-processing GitBook PDF made easy",
  "engines": {
    "gitbook": ">=2.5.0"
  },
  "gitbook": {
    "properties": {
      "background": {
        "title": "Background",
        "description": "Set a background, either using an image, a texture, or a hex color code.",
        "type": "object",
        "required": false,
        "properties": {
          "image": {
            "type": "string",
            "pattern": "(jpe?g|JPE?G|png|PNG|gif|GIF|bmp|BMP|pdf|PDF)$",
            "required": true
          },
          "position": {
            "type": "string",
            "default": "Center",
            "pattern": "^((center)|(((n|N)orth|(s|S)outh){0,1}((e|E)ast|(w|W)est){0,1})){1}$"
          },
          "offset": {
            "type": "string",
            "default": "0x0",
            "pattern": "^\\d+((x|X)\\d+)?$"
          },
          "fill": {
            "oneOf": [
              {
                "type": "boolean",
                "default": false
              },
              {
                "type": "string",
                "default": "#FFFFFF"
              }
            ]
          },
          "opacity": {
            "type": "number",
            "minimum": 0,
            "maximum": 1,
            "default": 1
          }
        }
      }
    }
  },
